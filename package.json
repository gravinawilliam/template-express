{
  "name": "template-express",
  "version": "0.0.0",
  "main": "dist/main/index.js",
  "repository": "git@github.com:gravinawilliam/template-express.git",
  "author": "William Gravina <dev.gravinawilliam@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.16.0"
  },
  "scripts": {
    "build": "rimraf dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "check:format": "prettier --check .",
    "check:lint": "eslint . --ext .js,.ts --color",
    "check:spelling": "cspell \"{README.md,TODO.md,.github/*.md,src/**/*.ts}\"",
    "check:staged": "lint-staged",
    "check:types": "tsc --pretty --noEmit",
    "commit": "cz",
    "coverage:view": "http-server -p 9003 ./coverage/lcov-report -o",
    "fix:format": "prettier --check --write --ignore-unknown .",
    "fix:lint": "npm run check:lint -- --fix",
    "prepare": "husky install",
    "start:local": "cross-env NODE_ENV=LOCAL ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --clear --ignore-watch node_modules --respawn src/main/index.ts",
    "start:prod": "cross-env NODE_ENV=PRODUCTION node ./dist/main/index.js",
    "start": "node ./dist/main/index.js",
    "test": "jest --colors --no-cache --passWithNoTests --detectOpenHandles --runInBand",
    "test:coverage": "yarn test --coverage --silent",
    "test:staged": "yarn test --findRelatedTests",
    "test:watch": "yarn test --watch",
    "update:deps": "ncu"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@cspell/dict-bash": "^2.0.3",
    "@cspell/dict-companies": "^2.0.7",
    "@cspell/dict-es-es": "^2.2.0",
    "@cspell/dict-filetypes": "^2.1.0",
    "@cspell/dict-html": "^3.0.2",
    "@cspell/dict-lorem-ipsum": "^2.0.0",
    "@cspell/dict-node": "^3.0.1",
    "@cspell/dict-npm": "^3.0.1",
    "@cspell/dict-software-terms": "^2.1.11",
    "@cspell/dict-typescript": "^2.0.0",
    "@swc/core": "^1.2.215",
    "@swc/jest": "^0.2.21",
    "@types/cookie-parser": "^1.4.3",
    "@types/dotenv-defaults": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/figlet": "^1.5.4",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.5",
    "@types/node-emoji": "^1.8.1",
    "@types/source-map-support": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "commitizen": "^4.2.4",
    "cspell": "^6.2.3",
    "cz-emoji-conventional": "^1.0.1",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.2.6",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^43.0.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "get-tsconfig": "^4.2.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-mock-extended": "^2.0.7",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^15.3.4",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "ts-jest": "^28.0.6",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv-defaults": "^5.0.2",
    "dotenv-expand": "^8.0.3",
    "endent": "^2.1.0",
    "express": "^4.18.1",
    "figlet": "^1.5.2",
    "helmet": "^5.1.0",
    "http-graceful-shutdown": "^3.1.7",
    "morgan": "^1.10.0",
    "node-emoji": "^1.11.0",
    "read-pkg": "5.2.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "prisma": {
    "schema": "src/infrastructure/database/prisma/schema.prisma"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
